# This would scan all the private classes and members too
# javadoc -private -excludeclass private -excludemember private 

# Remember to set the sourcepath to find the Java source files on your system
export SRCPATH=/home/matt/jdktests/141

/usr/java1.4/j2sdk1.4.1_01/bin/javadoc -J-Xmx256m -doclet jdiff.JDiff -docletpath ../lib/jdiff.jar:../lib/xerces.jar -source 1.4 -apiname J2SE1.4.1 -sourcepath .:$SRCPATH @jdk141pkglist.txt
